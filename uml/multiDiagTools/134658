    class 136322 "zmsFrame"
      visibility package 
      nactuals 1
      actual class class_ref 134914 // mdtBuffer
        rank 0 explicit_value "char"
      cpp_decl "${comment}${template}class ${name}${inherit} {
${members}};
${inlines}
"
      java_decl ""
      php_decl ""
      python_2_2 python_decl ""
      idl_decl ""
